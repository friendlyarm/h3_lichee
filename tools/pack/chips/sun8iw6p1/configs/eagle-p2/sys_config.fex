;H8 Box application
;---------------------------------------------------------------------------------------------------------
; 说明： 脚本中的字符串区分大小写，用户可以修改"="后面的数值，但是不要修改前面的字符串
; 描述gpio的形式：Port:端口+组内序号<功能分配><内部电阻状态><驱动能力><输出电平状态>
;---------------------------------------------------------------------------------------------------------

[product]
version = "100"
machine = "eagle_p1"
board_serial = 2

;---------------------------------------------------------------------------------------------------------
; eraseflag: 是否擦除量产, 1:擦除量产(极度建议使用), 0:不擦除量产
; next_work: usb量产工具(PhoenixUSBpro)量产完成工作状态,2:重启, 3:关机(极度建议使用), 4:重复量产
; secure_without_OS: 是否存在secure os系统, 1:不存在, 0:存在
; secure_bit: 是否需要烧写secure bit, 1:需要, 0:不需要
;---------------------------------------------------------------------------------------------------------
[platform]
eraseflag   = 1
next_work	= 3
secure_without_OS = 1
secure_bit  = 0

;----------------------------------------------------------------------------------
;   system configuration
;   ?
;dcdc1_vol	---set dcdc1 voltage,mV,1600-3400,100mV/step
;dcdc2_vol	---set dcdc2 voltage,mV,600-1540,20mV/step
;dcdc3_vol	---set dcdc3 voltage,mV,600-1860,20mV/step
;dcdc4_vol	---set dcdc4 voltage,mV,600-1540,20mV/step
;dcdc5_vol	---set dcdc5 voltage,mV,1000-2550,50mV/step
;aldo2_vol	---set aldo2 voltage,mV,700-3300,100mV/step
;aldo3_vol	---set aldo2 voltage,mV,700-3300,100mV/step
;----------------------------------------------------------------------------------
[target]
boot_clock   	= 1008
storage_type    = -1
burn_key		= 0

[charging_type]
charging_type = 1
;---------------------------------------------------------------------------------
; uboot key detect enable
; 当keyen_flag = 1 时，支持按键检测
; 当keyen_flag = 0 时，不支持
;---------------------------------------------------------------------------------
[key_detect_en]
keyen_flag = 0

;---------------------------------------------------------------------------------
;	used 1: box 0:pad
;	start_type 1: 直接启动系统 0: 上电不允许直接启动系统 
;	irkey_used 0:禁用ir key  1:启用ir key 
;	pmukey_used 0:禁用pmu power key  1:启用pmu power key 
;	pmukey_num : then index of gpio, only support PL,if pmukey_used=0, this uneless
;	led_power  : 1: on, 0: off, the default is 0
;	led_state  : 1: on, 0: off, the control is axpgpio0
;---------------------------------------------------------------------------------
[box_start_os]
used = 1
start_type = 1
irkey_used = 1
pmukey_used = 0
pmukey_num = 0
led_power = 0
led_state = 0

;----------------------------------------------------------------------------------
;
;  各路电压输出语法说明：
;
;  电压名称  = 100XXXX  : 表示把该路电压设置为XXXX指定的电压值，同时打开输出开关
;  电压名称  = 000XXXX  : 表示把该路电压设置为XXXX指定的电压值，同时关闭输出开关，当有需要时由内核驱动打开
;  电压名称  = 0        : 表示关闭该路电压输出开关，不修改原有的值
;
; *** H8 proto board ***
; dcdc1 -- nand/emmc/sd/usb0/ac100
; dcdc2 -- cpua
; dcdc3 -- cpub
; dcdc4 -- gpu
; dcdc5 -- dram (ddr3=1500mv, ddr3L=1350mv, lpddr2/lpddr3=1200mv)
; dcdc6 -- sys/usb0/hdmi
; aldo1 -- efuse/hdmi
; aldo2 -- pll/lpddr/drampll
; aldo3 -- avcc/pl
; fldo2 -- cpus
; dldo1 -- cvbs/av
;----------------------------------------------------------------------------------
[power_sply]
dcdc1_vol                  = 1003000
dcdc2_vol                  = 1000900
dcdc3_vol                  = 1000900
dcdc4_vol                  = 1000900
;dcdc5_vol                  = 1001500
dcdc6_vol                  = 1000900
aldo1_vol                  = 1001800
aldo2_vol                  = 1001800
aldo3_vol                  = 1003000
fldo2_vol                  = 1000900
dldo1_vol                  = 0003300
dldo4_vol                  = 1003300

[power_sply_ext]
dcdc1_vol                  = 1003000
dcdc2_vol                  = 1000900
dcdc3_vol                  = 1000900
dcdc4_vol                  = 1000900
;dcdc5_vol                  = 1001500
dcdc6_vol                  = 1000900
aldo1_vol                  = 1003300
aldo2_vol                  = 1001800
aldo3_vol                  = 1003000
fldo2_vol                  = 1000900
dldo1_vol                  = 1003300
dldo3_vol                  = 1002500

;---------------------------------------------
;
; normal config: eg. px_bias = "pmu_name:supply_name:voltage"
;
;                    pmu_name = axp809, axp806
;                    supply_name = dcdc1, dcdc2, aldo1, gpio1ldo, etc...
;
; special config: eg. px_bias = "floating"
;                    when the gpio is float, use it
;                    and the register will ignore it(use default value)
;
; special config: eg. px_bias = "constant:voltage"
;                    when the gpio connect a constant voltage, use it
;                    and the register will set a matching value
;
;---------------------------------------------
[gpio_bias]
;pa_bias         = "axp81x:dcdc1:3000"
pb_bias         = "axp81x:dcdc1:3000"
pc_bias         = "axp81x:dcdc1:3000"
pd_bias         = "axp81x:dldo4:3000"
pe_bias         = "axp81x:dcdc1:3000"
pf_bias         = "axp81x:dcdc1:3000"
pg_bias         = "axp81x:dcdc1:3000"
ph_bias         = "axp81x:dcdc1:3000"
pl_bias         = "axp81x:aldo3:3000"
[gpio_bias_ext]
;pa_bias         = "axp81x:dcdc1:3000"
pb_bias         = "axp81x:dcdc1:3000"
pc_bias         = "axp81x:dcdc1:3000"
pd_bias         = "axp81x:dcdc1:3000"
pe_bias         = "axp81x:dcdc1:3000"
pf_bias         = "axp81x:dcdc1:3000"
pg_bias         = "axp81x:dcdc1:3000"
ph_bias         = "axp81x:dcdc1:3000"
pl_bias         = "axp81x:aldo3:3000"
;-------------------------------------------------------------------------------
;	sprite_work_delay	:
;	 sprite_err_delay	:
;		 sprite_gpio0	：	卡量产、一键recovery状态指示灯
;			next_work	： 卡量产完成，工作状态(默认关机)
;-------------------------------------------------------------------------------
[card_boot]
logical_start   = 40960
sprite_work_delay	= 500
sprite_err_delay	= 200
sprite_gpio0    = port:power0<1><default><default><default>
next_work       = 3

;-------------------------------------------------------------------------------
; 	boot阶段上电初始化GPIO
;		used		:模块使能端     置1：开启模块   置0：关闭模块
;		gpiox ：上电初始化gpio （名称自定，但不能重复，并且GPIO允许可以多个）
;		PH06  : 系统显示LED GPIO
;-------------------------------------------------------------------------------
[boot_init_gpio]
used	=	1
gpio0	=  port:power0<1><default><default><1>

;----------------------------------------------------------------------------------
;	used: 模块使能端     1：开启模块   0：关闭模块
;	mode: 模式选择		 1：一键进入OTA升级		2：一键恢复（通过sysrecovery分区来恢复）  其他值：无效
;	recovery_key ： 按键配置  （例如：recovery_key= port:PH16<0><default>）
;----------------------------------------------------------------------------------
[recovery_para]
used =	1
mode =	2
recovery_key = port:PL09<0><default><default><default>


;---------------------------------------------------------------------------------------------------------
; if 1 == standby_mode, then support super standby;
; else, support normal standby.
;---------------------------------------------------------------------------------------------------------
[pm_para]
standby_mode		= 1

[card0_boot_para]
card_ctrl       = 0
card_high_speed = 1
card_line       = 4
sdc_d1          = port:PF00<2><1><2><default>
sdc_d0          = port:PF01<2><1><2><default>
sdc_clk         = port:PF02<2><1><2><default>
sdc_cmd         = port:PF03<2><1><2><default>
sdc_d3          = port:PF04<2><1><2><default>
sdc_d2          = port:PF05<2><1><2><default>

[card2_boot_para]
card_ctrl       = 2
card_high_speed = 1
card_line       = 8
sdc_cmd         = port:PC06<3><1><2><default>
sdc_clk         = port:PC05<3><1><2><default>
sdc_d0          = port:PC08<3><1><2><default>
sdc_d1          = port:PC09<3><1><2><default>
sdc_d2          = port:PC10<3><1><2><default>
sdc_d3          = port:PC11<3><1><2><default>
sdc_d4          = port:PC12<3><1><2><default>
sdc_d5          = port:PC13<3><1><2><default>
sdc_d6          = port:PC14<3><1><2><default>
sdc_d7          = port:PC15<3><1><2><default>
sdc_2xmode   	= 1
sdc_ddrmode		= 1


[twi_para]
twi_port        = 0
twi_scl         = port:PH14<2><default><default><default>
twi_sda         = port:PH15<2><default><default><default>

[uart_para]
uart_debug_port = 0
uart_debug_tx   = port:PB09<2><1><default><default>
uart_debug_rx   = port:PB10<2><1><default><default>

[force_uart_para]
force_uart_port  = 0
force_uart_tx    = port:PB09<2><1><default><default>
force_uart_rx    = port:PB10<2><1><default><default>

[jtag_para]
jtag_enable     = 1
jtag_ms         = port:PB00<3><default><default><default>
jtag_ck         = port:PB01<3><default><default><default>
jtag_do         = port:PB02<3><default><default><default>
jtag_di         = port:PB03<3><default><default><default>

[clock]
pll_ve           = 432
;pll_periph       = 600
pll_gpu          = 432
pll_hsic         = 480
pll_de           = 504
pll_video1       = 297

;*****************************************************************************
;sdram configuration
;
; if set ddr type auto detect, use dram_tpr7 to set dram clk.
; for example: if dram_tpr13[13:13] = 1, it use
; dram_tpr7[29:20] to set ddr3/ddr3L clk,
; dram_tpr7[19:10] to set lpddr2 clk,
; dram_tpr7[9:0] to lpddr3 clk.
;
;*****************************************************************************
[dram_para]
dram_clk        = 672
dram_type       = 7
dram_zq         = 0x3bfb
dram_odt_en     = 1
dram_para1      = 0x10E40000
dram_para2      = 0x0000
dram_mr0        = 0x1840
dram_mr1        = 0x40
dram_mr2        = 0x8
dram_mr3        = 0x2
dram_tpr0       = 0x0048A192
dram_tpr1       = 0x01B1B18d
dram_tpr2       = 0x00076052
dram_tpr3       = 0
dram_tpr4       = 0
dram_tpr5       = 0
dram_tpr6       = 0
dram_tpr7       = 0x2a0782a0
dram_tpr8       = 1
dram_tpr9       = 0x05008888
dram_tpr10      = 0x00008888
dram_tpr11      = 0x05005
dram_tpr12      = 0
dram_tpr13      = 0x2000

;----------------------------------------------------------------------------------
;os life cycle para configuration
;----------------------------------------------------------------------------------

;------------------------------------------------------------------------------;
; 10/100/100Mbps Ethernet MAC Controller Configure                             ;
;------------------------------------------------------------------------------;
;   配置选项：                                                                 ;
;   gmac_used  ---  1: gmac used, 0: not used                                  ;
;   gmac_powerx --  A[:B] A: axp channel, B: voltage value                     ;
;------------------------------------------------------------------------------;
;         MII        RGMII        MII        RGMII        MII        RGMII     ;
;PD02~05  *            *    PD15   *                PD22   *           *       ;
;   PD06  *            *    PD16                    PD23   *           *       ;
;   PD07  *            *    PD17                    PD24                       ;
;   PD08                    PD18   *           *    PD25                       ;
;   PD09                    PD19   *           *    PD26                       ;
;   PD10  *                 PD20   *                PD26                       ;
;PD11~14  *            *    PD21   *           *    PD28                       ;
;------------------------------------------------------------------------------;

[gmac0]
gmac_used          = 1
gmac_mode          = "MII"
gmac_rxd3          = port:PD02<2><default><3><default>
gmac_rxd2          = port:PD03<2><default><3><default>
gmac_rxd1          = port:PD04<2><default><3><default>
gmac_rxd0          = port:PD05<2><default><3><default>
gmac_rxclk         = port:PD06<2><default><3><default>
gmac_rxdv          = port:PD07<2><default><3><default>
;gmac_rxerr         = port:PD10<2><default><3><default>
gmac_txd3          = port:PD11<2><default><3><default>
gmac_txd2          = port:PD12<2><default><3><default>
gmac_txd1          = port:PD13<2><default><3><default>
gmac_txd0          = port:PD14<2><default><3><default>
;gmac_crs           = port:PD15<2><default><3><default>
gmac_txclk         = port:PD18<2><default><3><default>
gmac_txen          = port:PD19<2><default><3><default>
gmac_txerr         = port:PD20<2><default><3><default>
gmac_col           = port:PD21<2><default><3><default>
gmac_mdc           = port:PD22<2><default><3><default>
gmac_mdio          = port:PD23<2><default><3><default>
gmac_power1        = "vcc-ephy-io:3300000"
gmac_power2        =
gmac_power3        =



;---------------------------------------------------------------------------------------------------------
; wakeup_src_para:
;	sometimes, u would like to add more wakeup src in standby mode, these para will be
;	help;
;	u need to make sure the standby mode support the wakeup src. Also, some hw
;	condition must be guaranteed.
;	including:
;	cpu_en: power on or off.
;		1: mean power on
;		0: mean power off
;	cpu_freq: indicating lowest freq. unit is Mhz;
;	dram selfresh_en: selfresh or not.
;		1: enable enter selfresh
;		0: disable enter selfresh
;	dram_pll: if not enter selfresh, indicating lowest freq. unit is Mhz;
;	wakeup_src: to make the scenario work, the wakeup src is needed.
;---------------------------------------------------------------------------------------------------------
[wakeup_src_para]
cpu_en              = 0
cpu_freq            = 48
; (cpu:apb:ahb)
pll_ratio           = 0x111
dram_selfresh_en    = 1
dram_freq           = 36
wakeup_src0         =
;wakeup_src_wl       = port:PL03<4><default><default><0>

