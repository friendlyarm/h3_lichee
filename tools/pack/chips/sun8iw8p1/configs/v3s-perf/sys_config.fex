vip_dev0_stby_mode       = 0
vip_dev0_vflip           = 1
vip_dev0_hflip           = 1
vip_dev0_iovdd           = ""
vip_dev0_iovdd_vol       = 3300000
vip_dev0_avdd            = ""
vip_dev0_avdd_vol        = 3300000
vip_dev0_dvdd            = ""
vip_dev0_dvdd_vol        = 3300000
vip_dev0_afvdd           = ""
vip_dev0_afvdd_vol       = 2800000
vip_dev0_power_en        = 
