lcd_x               = 800
lcd_y               = 480
lcd_width           = 108
lcd_height          = 64
lcd_dclk_freq       = 30
lcd_pwm_used        = 1
lcd_pwm_ch          = 0
lcd_pwm_freq        = 50000
lcd_pwm_pol         = 1
lcd_pwm_max_limit   = 255
lcd_hbp             = 46
lcd_ht              = 1056
lcd_hspw            = 2
lcd_vbp             = 23
lcd_vt              = 1050
lcd_vspw            = 2
lcd_frm             = 0
lcd_hv_clk_phase    = 0
lcd_hv_sync_polarity = 0
lcd_gamma_en        = 0
lcd_bright_curve_en = 1
lcd_cmap_en         = 0

lcdgamma4iep        = 22

